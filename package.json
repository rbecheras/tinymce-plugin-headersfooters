{
  "name": "tinymce-plugin-headersfooters",
  "version": "1.0.0-rc.6",
  "description": "A plugin for tinymce WYSIWYG HTML editor that allow to insert headers and footers - requires tinymce-plugin-paginate",
  "main": "plugin.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "commander": "^2.9.0",
    "esdoc": "^1.0.3",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.8.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.1",
    "grunt-bump": "^0.6.0",
    "grunt-contrib-uglify": "^0.10.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-standard": "^2.0.0",
    "http-server": "^0.10.0",
    "inquirer": "^0.12.0",
    "load-grunt-tasks": "^3.3.0",
    "shelljs": "^0.6.0",
    "standard": "^10.0.3"
  },
  "scripts": {
    "build:clear": "rm -rf ./build",
    "build:babel": "./node_modules/.bin/babel --plugins transform-runtime src --out-dir build/lib --source-maps",
    "build": "yarn run build:clear && yarn run build:babel && grunt build && yarn run build:esdoc",
    "test": "echo \"WARNING: NO TEST AVAILABLE\" && exit 1",
    "build:esdoc": "./node_modules/.bin/esdoc -c esdoc.json",
    "serve:esdoc": "./node_modules/.bin/http-server -p 6060 build/docs/esdoc",
    "release-patch": "node ./scripts/release.js patch",
    "release-minor": "node ./scripts/release.js minor",
    "release-major": "node ./scripts/release.js major",
    "prerelease": "node ./scripts/release.js prerelease",
    "prerelease-patch": "node ./scripts/release.js prepatch",
    "prerelease-minor": "node ./scripts/release.js preminor",
    "prerelease-major": "node ./scripts/release.js premajor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:rbecheras/tinymce-plugin-headersfooters.git"
  },
  "keywords": [
    "tinymce",
    "plugin",
    "tinymce",
    "plugin",
    "headers",
    "footers"
  ],
  "author": "RÃ©mi Becheras <rbecheras@sirap.fr>",
  "license": "LGPL-2.1",
  "bugs": {
    "url": "https://github.com/rbecheras/tinymce-plugin-headersfooters/issues"
  },
  "homepage": "https://github.com/rbecheras/tinymce-plugin-headersfooters#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules/**"
    ]
  }
}
